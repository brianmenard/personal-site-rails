<body>
<a name="home"></a>
    
    <nav class='navbar navbar-default navbar-fixed-top navbar-custom'>
      <div class="container-fluid">
        <!-- collapse to button -->
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span> 
          </button>
        <%= link_to "Brian Menard", root_path(anchor: 'home'), class:"navbar-brand"%>
    </div>
      <!-- end collapse to button -->
        <div class='collapse navbar-collapse' id='myNavbar'>
          <ul class="nav navbar-nav">
            <li><%= link_to('Home', root_path(anchor: 'home')) %></li>
            <li><%= link_to('Skills', root_path(anchor: 'skills')) %></li>
            <li><%= link_to('Portfolio', root_path(anchor: 'portfolio')) %></li>
            <!-- <li><a href="#blog">Blog</a></li> -->
            <!-- <li><a href="#about">About</a></li> -->
            <li><%= link_to('Contact', root_path(anchor: 'contact'), class:"hidden-xs") %></li>
            <li><%= link_to('Contact & Social', root_path(anchor: 'home'), class:"menuLink visible-xs") %></li>
            <!-- IF ANY MORE LINKS ARE ADDED, ADD THEM TO SMOOTHSCROLL -->
          </ul>
          <ul class='nav navbar-nav navbar-right hidden-xs'>
            <li><a href='https://github.com/brianmenard' target="_blank">gitHub</a></li>
            <li><a href='https://www.linkedin.com/in/brianmenard' target="_blank">LinkedIn</a></li>
            <li><a href='https://webdevbrian.wordpress.com/' target="_blank">WordPress</a></li>
            <li><%= link_to 'Resume', asset_path('DevDesignResume.pdf'), target:'blank' %></li>
          </ul>
        </div>
      </div>
    </nav>


<%= yield %>


  <!-- START FOOTER -->
    <div class="container-fluid footer">
      <div class="row padLeft padRight padTop padBottom">
        <div class="col-xs-2"></div>
        
          <div class="col-md-3 subtext darkRed">
              Â© 2016 Brian Menard, unless otherwise noted.    
          </div>
        
          <div class="col-md-4 subtext darkRed">
            Designed and developed by Brian Menard. Built with BootStrap and SmoothScroll.  
            Fonts and icons courtesy of Google.
          </div>
          <div class='col-xs-3'></div>
        </div>
      </div>
  <!-- END FOOTER -->

    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <%= javascript_include_tag('smoothScroll.js') %>
    <%= javascript_include_tag('horizontalScroll.js') %>
    <%= javascript_include_tag('hideMenu.js') %>
    <%= javascript_include_tag('buttonUpdater.js') %>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>

